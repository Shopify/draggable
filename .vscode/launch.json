{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Tests",
      "program": "${workspaceRoot}/node_modules/jest/bin/jest.js",
      "args": [
          "--config",
          "config.json",
          "-i",
          "--watchAll"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": [
          "${workspaceRoot}/dist/**/*"
      ],
    }
  ]
}
